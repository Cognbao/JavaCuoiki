<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.test.controller.RegisterController" spacing="10"
      alignment="CENTER">
    <children>
        <TextField fx:id="usernameField" promptText="Username"/>
        <PasswordField fx:id="passwordField" promptText="Password"/>
        <PasswordField fx:id="confirmPasswordField" promptText="Confirm Password"/>
        <Button text="Register" fx:id="registerButton" onAction="#handleRegister"/>
    </children>
</VBox>
